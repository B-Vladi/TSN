	<!-- Подключение общих блоков -->
	<tsn:render file="common.xml" />

	<!DOCTYPE html>
	<html>
		<head>
			<!-- Результирующий код подключения стилей -->
			<tsn:render block="CSS" />
		</head>
		<body>
			<div class="wrapper">
				<!-- Результирующий код страницы -->
				<tsn:render block="CONTENT" />
